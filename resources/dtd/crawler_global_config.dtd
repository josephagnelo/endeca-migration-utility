<!ELEMENT CRAWLER_GLOBAL_CONFIG (DEBUG_DIRECTORY, OUTPUT_FORMAT, PROXY_SERVER, PROXY_SERVER_PORT, MEMORY_SETTING, CERTIFICATES)>

<!ELEMENT DEBUG_DIRECTORY EMPTY >
<!ATTLIST DEBUG_DIRECTORY
  NAME CDATA #IMPLIED
  SAVE (TRUE | FALSE) "FALSE"
>

<!ELEMENT OUTPUT_FORMAT EMPTY >
<!ATTLIST OUTPUT_FORMAT VALUE (BINARY | XML) #REQUIRED>
<!ATTLIST OUTPUT_FORMAT COMPRESSED (TRUE | FALSE) #REQUIRED>

<!ELEMENT PROXY_SERVER EMPTY >
<!ATTLIST PROXY_SERVER VALUE CDATA #REQUIRED>

<!ELEMENT PROXY_SERVER_PORT EMPTY >
<!ATTLIST PROXY_SERVER_PORT VALUE CDATA #REQUIRED>

<!ELEMENT MEMORY_SETTING EMPTY >
<!ATTLIST MEMORY_SETTING VALUE CDATA #REQUIRED>

<!ELEMENT CERTIFICATES (CERTIFICATE*)>

<!ELEMENT CERTIFICATE (#PCDATA)>
<!ATTLIST CERTIFICATE NAME CDATA #REQUIRED>
